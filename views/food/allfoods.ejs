<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All</title>
</head>
<body>
    <%- include("../partials/navbar.ejs")  %> 

    <h1>All Food</h1>


    <% let count = 1 %> 
    <% allFood.forEach( food => { %>

        <div style="background: red;margin: 10px">
            <p>Number: <%= count %> </p>
            <p>Name: <%= food.name %> </p>
            <p>price: <%= food.price %></p>
            <% count++ %> 
        </div>

    <% } ) %> 

    


</body>
</html>